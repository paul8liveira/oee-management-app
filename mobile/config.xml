<?xml version='1.0' encoding='utf-8'?>
<widget id="site.smup.oee_app" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>StartMeUp</name>
    <description>
        StartMeUp - Monitoramento Online de Produção.
    </description>
    <author email="tec@smup.site" href="https://www.smup.site/">
        paul8liveira
    </author>
    <content src="index.html" />    
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="https://oee-server.appspot.com" />
    <preference name="StatusBarBackgroundColor" value="#314289" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="res/icon/android/ldpi.png" density="ldpi" />
        <icon src="res/icon/android/mdpi.png" density="mdpi" />
        <icon src="res/icon/android/hdpi.png" density="hdpi" />
        <icon src="res/icon/android/xhdpi.png" density="xhdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="DisallowOverscroll" value="true" />
    </platform>
    <plugin name="cordova-plugin-statusbar" spec="~2.4.2" />
    <plugin name="cordova-plugin-fcm" spec="~2.1.2" />
</widget>